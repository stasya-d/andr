<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="DobrodeyTheme" parent="@android:style/Theme.Black">
        <item name="android:colorBackground">#FFFF6E6E</item>
        <item name="android:textColor">#F0E68C</item>
        <item name="android:buttonStyle">@style/DobrodeyThemeButton</item>
    </style>

    <style name="DobrodeyThemeButton" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/hbutton</item>
    </style>

    <style name="MyTextStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textSize">26dip</item>
    </style>

    <style name="EnterStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#008080</item>
        <item name="android:textColor">#808080</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textSize">20dip</item>
    </style>
</resources>